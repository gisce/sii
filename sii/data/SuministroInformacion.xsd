	<!-- Datos de un apunte correspondiente al libro de registro de determinadas operaciones intracomunitarias. -->
	<complexType name="OperacionIntracomunitariaType">
		<annotation>
			<documentation xml:lang="es"> Apunte correspondiente al libro de operaciones intracomunitarias. </documentation>
		</annotation>
		<sequence>
			<element name="TipoOperacion">
				<simpleType>
					<restriction base="string">
						<enumeration value="A">
							<annotation>
								<documentation xml:lang="es">envío o recepción de bienes para la realización de los informes parciales o trabajos mencionados en el artículo 70, apartado uno, número 7º, de la Ley del Impuesto (Ley 37/1992).</documentation>
							</annotation>
						</enumeration>
						<enumeration value="B">
							<annotation>
								<documentation xml:lang="es">transferencias de bienes y las adquisiciones intracomunitarias de bienes comprendidas en los artículos 9, apartado 3º, y 16, apartado 2º, de la Ley del Impuesto (Ley 37/1992).</documentation>
							</annotation>
						</enumeration>
					</restriction>
				</simpleType>
			</element>
			<element name="ClaveDeclarado">
				<simpleType>
					<restriction base="string">
						<enumeration value="D">
							<annotation>
								<documentation xml:lang="es">DESTINATARIO</documentation>
							</annotation>
						</enumeration>
						<enumeration value="R">
							<annotation>
								<documentation xml:lang="es">REMITENTE</documentation>
							</annotation>
						</enumeration>
					</restriction>
				</simpleType>
			</element>
			<element name="EstadoMiembro" type="sii:CountryMiembroType"/>
			<element name="PlazoOperacion" minOccurs="0">
				<simpleType>
					<restriction base="string">
						<pattern value="\d{1,3}"/>
					</restriction>
				</simpleType>
			</element>
			<element name="DescripcionBienes" type="sii:TextMax40Type"/>
			<element name="DireccionOperador" type="sii:TextMax120Type"/>
			<element name="FacturasODocumentacion" type="sii:TextMax150Type" minOccurs="0"/>
		</sequence>
	</complexType>
